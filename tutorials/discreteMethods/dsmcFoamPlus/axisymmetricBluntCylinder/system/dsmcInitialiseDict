FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      dsmcInitialiseDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

configurations
(

	configuration
    {
        type			dsmcMeshFill;

		numberDensities
		{
			Ar              1.0e21;
		};

		translationalTemperature     	100;
		rotationalTemperature			100;
		vibrationalTemperature			100;
        electronicTemperature           0;

		velocity        (1000 0 0);
	}

);


